  "scripts": {
    "clean-dist": "rimraf dist/*",
    "start": "yarn run start-dev",
    "postversion": "react-native-version  --never-amend",
    "bump-version": "gulp increaseVersion",
    "build:project2": "env-cmd -f config/environment/project2.prod.env webpack -p --config=config/webpack/project2.prod ",
    "build:dev:project2": "env-cmd -f config/environment/project2.test.prod.env webpack -p  --config=config/webpack/project2.test.prod",
    "build:project1": "env-cmd -f config/environment/project1.prod.env webpack -p --config=config/webpack/project1.prod ",
    "build:dev:project1": "env-cmd -f config/environment/project1.test.prod.env webpack -p --config=config/webpack/project1.test.prod ",
    "start-dev:project2": "env-cmd -f config/environment/project2.dev.env webpack-dev-server --http --config=config/webpack/project2.dev  --host 0.0.0.0 --port 9091",
    "start-dev:project1": "env-cmd -f config/environment/project1.dev.env webpack-dev-server --http --config=config/webpack/project1.dev  --host 0.0.0.0 --port 9092",
    "mobile-dev:android": "env-cmd -f config/environment/mobile1.dev.env react-native run-android --variant=psDebug ",
    "mobile:cyprus-dev:android": "env-cmd -f ./config/environment/cyprus_mobile.dev.env react-native run-android --variant=cyprusDebug",
    "mobile-dev:ios": "env-cmd -f config/environment/mobile1.dev.env react-native run-ios",
    "mobile-dev:ios:bundle": "react-native bundle --entry-file project/mobile/index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "mobile:ios:bundle": "react-native bundle --entry-file project/mobile/index.js --platform ios --dev false --bundle-output ios/main.jsbundle --assets-dest ios",
    "mobile:start": "env-cmd -f config/environment/mobile1.dev.env react-native start",
    "mobile:reset": "react-native start --reset-cache",
    "start-prod": "env-cmd -f config/environment/mobile1.prod.env yarn run build && node express.js",
    "devtools": "react-devtools",
    "test": "jest --watchAll --coverage --config=configs/jest.json",
    "fastlane:android:dev": "env-cmd -f config/environment/mobile1.dev.env fastlane android devDeploy",
    "fastlane:android:cyprus:dev": "env-cmd -f ./config/environment/cyprus_mobile.dev.env fastlane android devCyprusDeploy",
    "fastlane:android:prod": "env-cmd -f config/environment/mobile1.prod.env fastlane android prodDeploy",
    "fastlane:ios:dev": "env-cmd -f config/environment/mobile1.prod.env fastlane ios beta",
    "android-release": "cd android && env-cmd -f ../config/environment/mobile.dev.env gradlew assembleRelease",
    "open-as": "open -a \"Android Studio.app\"",
    "open-port": "adb reverse tcp:8081 tcp:8081"
  },
